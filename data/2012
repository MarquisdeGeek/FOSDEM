        <slug>opening_talk</slug>
        <title>Opening talk</title>
        <subtitle></subtitle>
        <track>Graph Processing Devroom</track>
        <type></type>
        <language></language>
        <abstract>Room opening talk</abstract>
        <description></description>
        <persons>
